{
  "name": "Sustainify",
  "description": "Information about the sustainability of a company",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "webNavigation",
    "scripting",
    "tabs",
    "background",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "48": "icon48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Sustainify"
  }
}
